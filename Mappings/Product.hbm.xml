<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="NHibernateTest" namespace="NHibernate.Models">
	<class name="Product" table="Products">
		<id name="ProductID">
			<generator class="native" />
		</id>

		<property name="ProductName"></property>
		<property name="SupplierID"></property>
		<property name="CategoryID"></property>
		<property name="QuantityPerUnit"></property>
		<property name="UnitPrice"></property>
		<property name="UnitsInStock"></property>
		<property name="UnitsOnOrder"></property>
		<property name="ReorderLevel"></property>
		<property name="Discontinued"></property>
	</class>

</hibernate-mapping>